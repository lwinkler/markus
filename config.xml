<!-- List all possibly used inputs-->
<Module name="MyCam1">
	<parameters>
	<param id="0" name="class">UsbCam</param>
        <param id="1" name="inputWidth">200</param>
        <param id="2" name="inputHeight">100</param>
        <param id="3" name="inputType">CV_8UC3</param>
        <param id="4" name="num">-1</param>	
	<param id="4" name="fps">1000</param>
</parameters>
</Module>

<Module name="MyCam2">
	<param id="0" name="class">UsbCam</param>
