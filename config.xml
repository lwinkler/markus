<?xml version="1.0" standalone="no" ?>

<ImageProcessors>
	<ImageProcessor name="ModuleDeTest">
		<parameter id="0" name="input">InFile1</parameter>
		<parameter id="2" name="module">ObjectTracker1</parameter>
	</ImageProcessor>
	<ImageProcessor name="ModuleDeTest2">
                <parameter id="0" name="input">MyCam1</parameter>
                <parameter id="2" name="module">FaceDetector1</parameter>
        </ImageProcessor>
        <ImageProcessor name="ModuleDeTest3">
                <parameter id="0" name="input">MyCam1</parameter>
                <parameter id="2" name="module">SlitCamera2</parameter>
        </ImageProcessor>

</ImageProcessors>


<Manager>
	<parameter id="1" name="mode">benchmarkAA</parameter>
        <!--<parameter id="2" name="width">320</parameter>
	<parameter id="3" name="height">240</parameter>
        <parameter id="4" name="depth">8</parameter>
        <parameter id="5" name="channels">3</parameter>
	-->
</Manager>

<!-- List all possibly used inputs-->
<Input name="MyCam1">
	<parameter id="0" name="class">UsbCam</parameter>
        <parameter id="1" name="width">200</parameter>
        <parameter id="2" name="height">100</parameter>
        <parameter id="3" name="depth">8</parameter>
        <parameter id="4" name="channels">3</parameter>
        <parameter id="4" name="num">-1</parameter>	
	<parameter id="4" name="fps">1000</parameter>
</Input>

<Input name="MyCam2">
	<parameter id="0" name="class">UsbCam</parameter>
        <parameter id="1" name="width">200</parameter>
        <parameter id="2" name="height">100</parameter>
        <parameter id="3" name="depth">8</parameter>
        <parameter id="4" name="channels">3</parameter>
        <parameter id="4" name="num">1</parameter>	
        <parameter id="4" name="fps">10</parameter>
</Input>

<Input name="InFile1">
	<parameter id="0" name="class">VideoFileReader</parameter>
        <parameter id="1" name="width">200</parameter>
        <parameter id="2" name="height">100</parameter>
        <parameter id="3" name="depth">8</parameter>
        <parameter id="4" name="channels">3</parameter>
        <parameter id="4" name="file">input/test.avi</parameter>
	<parameter id="4" name="fps">10</parameter>
</Input>

<!-- List all possibly used modules-->

<Module name="ObjectTracker1">
	<parameter id="0" name="class">ObjectTracker</parameter>
        <parameter id="2" name="width">640</parameter>
        <parameter id="3" name="height">480</parameter>
        <parameter id="4" name="depth">8</parameter>
        <parameter id="5" name="channels">3</parameter>
	<parameter id="3" name="input_blur_size">3</parameter>
	<parameter id="1" name="background_alpha">0.01</parameter>
	<parameter id="2" name="foreground_thres">0.2</parameter>
	<parameter id="3" name="foreground_filter_size">3</parameter>
	<parameter id="4" name="fps">0</parameter>
</Module>
<Module name="SlitCamera1">
	<parameter id="0" name="class">SlitCamera</parameter>
        <parameter id="2" name="width">160</parameter>
        <parameter id="3" name="height">120</parameter>
        <parameter id="4" name="depth">8</parameter>
        <parameter id="5" name="channels">3</parameter>
	<parameter id="1" name="aperture">1</parameter>
	<parameter id="4" name="fps">0</parameter>
</Module>
<Module name="SlitCamera2">
	<parameter id="0" name="class">SlitCamera</parameter>
        <parameter id="2" name="width">640</parameter>
        <parameter id="3" name="height">480</parameter>
        <parameter id="4" name="depth">8</parameter>
        <parameter id="5" name="channels">3</parameter>
        <parameter id="1" name="aperture">1</parameter>
	<parameter id="4" name="fps">1</parameter>
</Module>
<Module name="FaceDetector1">
        <parameter id="0" name="class">FaceDetector</parameter>
        <parameter id="2" name="width">64</parameter>
        <parameter id="3" name="height">48</parameter>
        <parameter id="4" name="depth">8</parameter>
        <parameter id="5" name="channels">1</parameter>
        <parameter id="6" name="fps">10</parameter>
        <parameter id="7" name="minNeighbors">3</parameter>
        <parameter id="8" name="minFaceSide">0</parameter>
        <parameter id="9" name="scaleFactor">1.1</parameter>
        <parameter id="4" name="filterFile">modules/FaceDetector/lbpcascade_frontalface.xml</parameter>
</Module>
