			"type" : "bool"
		},
		"bShadowDetection" : 
		{
			"default" : false,
			"description" : "Enable shadow detection",
			"range" : "[0:1]",
			"type" : "bool"
		},
		"cached" : 
		{
			"default" : "NO_CACHE",
