<module name="VideoFileReader0" description="Read video stream from a video file">
	<parameters>
		<param name="auto_process">
			<type>bool</type>
			<value default='1' range='[1:1]'>1</value>
			<description>If yes the module processes with a timer at fixed fps, if no the module processes based on the time stamp of the input stream</description>
		</param>
		<param name="fps">
			<type>double</type>
			<value default='123' range='[0:1000]'>123</value>
			<description>Frames per seconds (processing speed)</description>
		</param>
		<param name="class">
			<type>string</type>
			<value default='VideoFileReader' range='[]'>VideoFileReader</value>
			<description>Class of the module (define the module's function)</description>
		</param>
		<param name="master">
			<type>string</type>
			<value default='' range='[]'></value>
			<description>Master module on which this module's processing is dependent. If empty, use all preceeding modules</description>
		</param>
		<param name="width">
			<type>int</type>
			<value default='640' range='[1:6400]'>640</value>
			<description>Width of the input</description>
		</param>
		<param name="height">
			<type>int</type>
			<value default='480' range='[1:4800]'>480</value>
			<description>Height of the input</description>
		</param>
		<param name="type">
			<type>imageType</type>
			<value default='CV_8UC1' range='[CV_16SC1,CV_16SC2,CV_16SC3,CV_16SC4,CV_16UC1,CV_16UC2,CV_16UC3,CV_16UC4,CV_32FC1,CV_32FC2,CV_32FC3,CV_32FC4,CV_32SC1,CV_32SC2,CV_32SC3,CV_32SC4,CV_64FC1,CV_64FC2,CV_64FC3,CV_64FC4,CV_8SC1,CV_8SC2,CV_8SC3,CV_8SC4,CV_8UC1,CV_8UC2,CV_8UC3]'>CV_8UC1</value>
			<description>Format of the input image</description>
		</param>
		<param name="cached">
			<type>cachedState</type>
			<value default='NO_CACHE' range='[DISABLED,NO_CACHE,READ_CACHE,WRITE_CACHE]'>NO_CACHE</value>
			<description>Format of the input image</description>
		</param>
		<param name="file">
			<type>string</type>
			<value default='in/input.mp4' range='[]'>in/input.mp4</value>
			<description>Name of the video file to read, with path</description>
		</param>
		<param name="loop">
			<type>bool</type>
			<value default='0' range='[0:1]'>0</value>
			<description>Loop on file</description>
		</param>
	</parameters>
	<inputs>
	</inputs>
	<outputs>
		<output id="0">
			<type>Image</type>
			<name>input</name>
			<description>Video stream</description>
		</output>
	</outputs>
</module>
