{
	"class" : "VideoFileReader",
	"description" : "Read video stream from a video file",
	"inputs" : 
	{
		"autoProcess" : 
		{
			"default" : true,
			"description" : "If yes the module processes with a timer at fixed fps, if no the module processes based on the time stamp of the input stream",
			"range" : "[1:1]",
			"type" : "bool"
		},
		"cached" : 
		{
			"default" : "NO_CACHE",
			"description" : "Format of the input image",
			"range" : "[DISABLED,NO_CACHE,READ_CACHE,WRITE_CACHE]",
			"type" : "cachedState"
		},
		"file" : 
		{
			"default" : "in/input.mp4",
			"description" : "Name of the video file to read, with path",
			"range" : {},
			"type" : "string"
		},
		"fps" : 
		{
			"default" : 0,
			"description" : "Frames per seconds (processing speed)",
			"range" : 
			{
				"max" : 1000,
				"min" : 0
			},
			"type" : "double"
		},
		"height" : 
		{
			"default" : 480,
			"description" : "Height of the input",
			"range" : 
			{
				"max" : 4800,
				"min" : 1
			},
			"type" : "int"
		},
		"loop" : 
		{
			"default" : false,
			"description" : "Loop on file",
			"range" : "[0:1]",
			"type" : "bool"
		},
		"master" : 
		{
			"default" : "",
			"description" : "Master module on which this module's processing is dependent. If empty, use all preceeding modules",
			"range" : {},
			"type" : "string"
		},
		"type" : 
		{
			"default" : "CV_8UC1",
			"description" : "Format of the input image",
			"range" : {},
			"type" : "imageType"
		},
		"width" : 
		{
			"default" : 640,
			"description" : "Width of the input",
			"range" : 
			{
				"max" : 6400,
				"min" : 1
			},
			"type" : "int"
		}
	},
	"outputs" : 
	{
		"image" : 
		{
			"default" : 
			{
				"height" : 480,
				"width" : 640
			},
			"description" : "Video stream",
			"id" : 0,
			"type" : "Image"
		}
	}
}