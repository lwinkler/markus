{
	"class": "VideoFileReader",
	"description": "Read video stream from a video file",
	"inputs": [
		{
			"class": "ParameterBool",
			"default": true,
			"description": "If yes the module processes with a timer at fixed fps, if no the module processes based on the time stamp of the input stream",
			"name": "autoProcess",
			"range": {
				"min": true
			}
		},
		{
			"class": "ParameterDouble",
			"default": 0.0,
			"description": "Frames per seconds (processing speed)",
			"name": "fps",
			"range": {
				"max": 1000.0,
				"min": 0.0
			}
		},
		{
			"class": "ParameterString",
			"default": "",
			"description": "Master module on which this module's processing is dependent. If empty, use all preceeding modules",
			"name": "master",
			"range": {}
		},
		{
			"class": "ParameterInt",
			"default": 640,
			"description": "Width of the input",
			"name": "width",
			"range": {
				"max": 6400,
				"min": 1
			}
		},
		{
			"class": "ParameterInt",
			"default": 480,
			"description": "Height of the input",
			"name": "height",
			"range": {
				"max": 4800,
				"min": 1
			}
		},
		{
			"class": "ParameterImageType",
			"default": "CV_8UC1",
			"description": "Format of the input image",
			"enum": [
				"CV_16SC1",
				"CV_16SC2",
				"CV_16SC3",
				"CV_16SC4",
				"CV_16UC1",
				"CV_16UC2",
				"CV_16UC3",
				"CV_16UC4",
				"CV_32FC1",
				"CV_32FC2",
				"CV_32FC3",
				"CV_32FC4",
				"CV_32SC1",
				"CV_32SC2",
				"CV_32SC3",
				"CV_32SC4",
				"CV_64FC1",
				"CV_64FC2",
				"CV_64FC3",
				"CV_64FC4",
				"CV_8SC1",
				"CV_8SC2",
				"CV_8SC3",
				"CV_8SC4",
				"CV_8UC1",
				"CV_8UC2",
				"CV_8UC3"
			],
			"name": "type",
			"range": {}
		},
		{
			"class": "ParameterCachedState",
			"default": "NO_CACHE",
			"description": "Format of the input image",
			"enum": [
				"DISABLED",
				"NO_CACHE",
				"READ_CACHE",
				"WRITE_CACHE"
			],
			"name": "cached",
			"range": {}
		},
		{
			"class": "ParameterString",
			"default": "in/input.mp4",
			"description": "Name of the video file to read, with path",
			"name": "file",
			"range": {}
		},
		{
			"class": "ParameterBool",
			"default": false,
			"description": "Loop on file",
			"name": "loop",
			"range": {}
		}
	],
	"outputs": [
		{
			"class": "StreamImage",
			"default": {
				"height": 480,
				"width": 640
			},
			"description": "Video stream",
			"name": "image",
			"range": {},
			"stream": true
		}
	]
}