<?xml version="1.0" encoding="UTF-8"?>
<module name="KeyPointsOrb" description="Extract key points of type Orb">
	<parameters>
		<param name="auto_process">
			<type>bool</type>
			<value min="0" max="1" default="0">0</value>
			<description>If yes the module processes with a timer at fixed fps, if no the module processes based on the time stamp of the input stream</description>
		</param>
		<param name="allow_unsync_input">
			<type>bool</type>
			<value min="0" max="1" default="0">0</value>
			<description>If yes the module accepts that its input can be on a different time stamp. Only relevant if the module has many inputs. Use at your own risks.</description>
		</param>
		<param name="fps">
			<type>double</type>
			<value min="0" max="1000" default="0">0</value>
			<description>Frames per seconds (processing speed)</description>
		</param>
		<param name="class">
			<type>string</type>
			<value default="">KeyPointsOrb</value>
			<description>Class of the module (define the module's function)</description>
		</param>
		<param name="width">
			<type>int</type>
			<value min="1" max="1280" default="640">640</value>
			<description>Width of the input</description>
		</param>
		<param name="height">
			<type>int</type>
			<value min="1" max="960" default="480">480</value>
			<description>Height of the input</description>
		</param>
		<param name="type">
			<type>imageType</type>
			<value default="CV_8UC1">CV_8UC1</value>
			<description>Format of the input image</description>
		</param>
		<param name="descriptor">
			<type>string</type>
			<value default=""></value>
			<description>The keypoint descriptor: ORB, BRIEF or Opponent. Leave empty for none.</description>
		</param>
		<param name="nb_features">
			<type>int</type>
			<value min="1" max="1000" default="500">500</value>
			<description></description>
		</param>
		<param name="scale_factor">
			<type>float</type>
			<value min="1" max="1.2" default="1.2">1.2</value>
			<description></description>
		</param>
		<param name="nb_levels">
			<type>int</type>
			<value min="1" max="10" default="8">8</value>
			<description></description>
		</param>
		<param name="edge_threshold">
			<type>int</type>
			<value min="2" max="100" default="31">31</value>
			<description></description>
		</param>
		<param name="first_level">
			<type>int</type>
			<value min="0" max="5" default="0">0</value>
			<description></description>
		</param>
		<param name="wta_k">
			<type>int</type>
			<value min="0" max="100" default="2">2</value>
			<description></description>
		</param>
	</parameters>
	<inputs>
		<input id="0">
			<type>Image</type>
			<name>image</name>
			<description>Video input</description>
		</input>
		<input id="1">
			<type>Objects</type>
			<name>objects</name>
			<description>Incoming objects</description>
		</input>
	</inputs>
	<outputs>
		<output id="0">
			<type>Objects</type>
			<name>output</name>
			<description>List of tracked object with KeyPoint and features</description>
		</output>
	</outputs>
</module>
