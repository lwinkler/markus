<?xml version="1.0" encoding="UTF-8"?>
<module name="KeyPointsMser" description="Extract key points of type Mser">
	<parameters>
		<param name="class">
			<type>string</type>
			<value default="">KeyPointsMser</value>
			<description>Class of the module (define the module's function)</description>
		</param>
		<param name="width">
			<type>int</type>
			<value min="1" max="6400" default="640">640</value>
			<description>Width of the input</description>
		</param>
		<param name="height">
			<type>int</type>
			<value min="1" max="4800" default="480">480</value>
			<description>Height of the input</description>
		</param>
		<param name="type">
			<type>imageType</type>
			<value default="CV_8UC1">CV_8UC1</value>
			<description>Format of the input image</description>
		</param>
		<param name="auto_process">
			<type>bool</type>
			<value min="0" max="1" default="0">0</value>
			<description>If yes the module processes with a timer at fixed fps, if no the module processes based on the time stamp of the input stream</description>
		</param>
		<param name="allow_unsync_input">
			<type>bool</type>
			<value min="0" max="1" default="0">0</value>
			<description>If yes the module accepts that its input can be on a different time stamp. Only relevant if the module has many inputs. Use at your own risks.</description>
		</param>
		<param name="fps">
			<type>double</type>
			<value min="0" max="1000" default="0">0</value>
			<description>Frames per seconds (processing speed)</description>
		</param>
		<param name="delta">
			<type>int</type>
			<value min="1" max="100" default="5">5</value>
			<description></description>
		</param>
		<param name="min_area">
			<type>int</type>
			<value min="0" max="100" default="60">60</value>
			<description></description>
		</param>
		<param name="max_area">
			<type>int</type>
			<value min="1" max="2147483647" default="14400">14400</value>
			<description></description>
		</param>
		<param name="max_variation">
			<type>float</type>
			<value min="0" max="1" default="0.25">0.25</value>
			<description></description>
		</param>
		<param name="min_diversity">
			<type>float</type>
			<value min="0" max="1" default="0.2">0.2</value>
			<description></description>
		</param>
		<param name="max_evolution">
			<type>int</type>
			<value min="1" max="1000" default="200">200</value>
			<description></description>
		</param>
		<param name="area_threshold">
			<type>double</type>
			<value min="1" max="2" default="1.01">1.01</value>
			<description></description>
		</param>
		<param name="min_margin">
			<type>double</type>
			<value min="0" max="1" default="0.003">0.003</value>
			<description></description>
		</param>
		<param name="edge_blur_size">
			<type>int</type>
			<value min="0" max="100" default="5">5</value>
			<description></description>
		</param>
	</parameters>
	<inputs>
		<input id="0">
			<type>Image</type>
			<name>image</name>
			<description>Video input</description>
		</input>
		<input id="1">
			<type>Objects</type>
			<name>objects</name>
			<description>Incoming objects</description>
		</input>
	</inputs>
	<outputs>
		<output id="0">
			<type>Objects</type>
			<name>output</name>
			<description>List of tracked object with KeyPoint and features</description>
		</output>
	</outputs>
</module>
