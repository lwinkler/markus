<?xml version="1.0" encoding="UTF-8"?>
<module name="Logger" description="This module takes a state as input and logs it to .srt file">
	<parameters>
		<param name="class">
			<type>string</type>
			<value default="">Logger</value>
			<description>Class of the module (define the module's function)</description>
		</param>
		<param name="width">
			<type>int</type>
			<value min="1" max="6400" default="640">640</value>
			<description>Width of the input</description>
		</param>
		<param name="height">
			<type>int</type>
			<value min="1" max="4800" default="480">480</value>
			<description>Height of the input</description>
		</param>
		<param name="type">
			<type>image type</type>
			<value default="CV_8UC1">CV_8UC1</value>
			<description>Format of the input image</description>
		</param>
		<param name="fps">
			<type>double</type>
			<value min="0" max="1000" default="10">10</value>
			<description>Frames per seconds (processing speed)</description>
		</param>
	</parameters>
	<inputs>
		<input id="0">
			<type>State</type>
			<name>input</name>
			<description>Input state to be logged</description>
		</input>
	</inputs>
	<outputs>
	</outputs>
</module>
