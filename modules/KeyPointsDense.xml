<?xml version="1.0" encoding="UTF-8"?>
<module name="KeyPointsDense" description="Extract key points of type Dense">
	<parameters>
		<param name="auto_process">
			<type>bool</type>
			<value min="0" max="1" default="0">0</value>
			<description>If yes the module processes with a timer at fixed fps, if no the module processes based on the time stamp of the input stream</description>
		</param>
		<param name="allow_unsync_input">
			<type>bool</type>
			<value min="0" max="1" default="0">0</value>
			<description>If yes the module accepts that its input can be on a different time stamp. Only relevant if the module has many inputs. Use at your own risks.</description>
		</param>
		<param name="fps">
			<type>double</type>
			<value min="0" max="1000" default="0">0</value>
			<description>Frames per seconds (processing speed)</description>
		</param>
		<param name="class">
			<type>string</type>
			<value default="">KeyPointsDense</value>
			<description>Class of the module (define the module's function)</description>
		</param>
		<param name="width">
			<type>int</type>
			<value min="64" max="6400" default="640">640</value>
			<description>Width of the input</description>
		</param>
		<param name="height">
			<type>int</type>
			<value min="48" max="4800" default="480">480</value>
			<description>Height of the input</description>
		</param>
		<param name="type">
			<type>imageType</type>
			<value default="CV_8UC1">CV_8UC1</value>
			<description>Format of the input image</description>
		</param>
		<param name="cached">
			<type>cachedState</type>
			<value default="NO_CACHE">NO_CACHE</value>
			<description>Format of the input image</description>
		</param>
		<param name="descriptor">
			<type>string</type>
			<value default=""></value>
			<description>The keypoint descriptor: ORB, BRIEF or Opponent. Leave empty for none.</description>
		</param>
		<param name="init_feature_scale">
			<type>float</type>
			<value min="1" max="255" default="1">1</value>
			<description></description>
		</param>
		<param name="feature_scale_levels">
			<type>int</type>
			<value min="1" max="2" default="1">1</value>
			<description>Number of levels of scaling</description>
		</param>
		<param name="feature_scale_mul">
			<type>float</type>
			<value min="0" max="1" default="0.1">0.1</value>
			<description>Multiplication factor between each level</description>
		</param>
		<param name="init_xy_step">
			<type>int</type>
			<value min="1" max="255" default="6">6</value>
			<description></description>
		</param>
		<param name="init_img_bound">
			<type>int</type>
			<value min="0" max="255" default="0">0</value>
			<description></description>
		</param>
		<param name="vary_xy_step_with_scale">
			<type>bool</type>
			<value min="0" max="1" default="1">1</value>
			<description>The grid node size is multiplied if true</description>
		</param>
		<param name="vary_img_bound_with_scale">
			<type>bool</type>
			<value min="0" max="1" default="0">0</value>
			<description>Size of image boundary is multiplied if true</description>
		</param>
	</parameters>
	<inputs>
		<input id="0">
			<type>Image</type>
			<name>image</name>
			<description>Video input</description>
		</input>
		<input id="1">
			<type>Objects</type>
			<name>objects</name>
			<description>Incoming objects</description>
		</input>
	</inputs>
	<outputs>
		<output id="0">
			<type>Objects</type>
			<name>output</name>
			<description>List of tracked object with KeyPoint and features</description>
		</output>
	</outputs>
</module>
